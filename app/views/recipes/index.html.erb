<% @recipes.each do |recipe| %>
  <%=cl_image_tag recipe.photo.key, height: 200, width: 300, crop: :fill  %>
  <br>
  <%=recipe.name%>
  <%=recipe.description%>
  <%=recipe.meal%>
  <%=recipe.cuisine%>
  <%=recipe.serves%>
  <%=recipe.cook_time%>
  <%=recipe.user.id%>
  <%= link_to 'More info', recipe_path(recipe.id), class: "button" %>
  <br>
<% end %>
<br>
<%= link_to "Create Recipe", new_recipe_path %>
